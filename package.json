{
  "name": "react-mdcw",
  "version": "0.1.1",
  "description": "React Components for Metarial Component Web",
  "author": "Choi Geonu",
  "license": "MIT",
  "main": "lib/",
  "repository": "https://github.com/Hardtack/react-mdcw.git",
  "dependencies": {
    "classnames": "^2.2.5",
    "immutable": "^3.8.1",
    "material-components-web": "^0.2.0",
    "react": "^15.0.0",
    "react-dom": "^15.4.2"
  },
  "devDependencies": {
    "babel": "^6.5.2",
    "babel-cli": "^6.18.0",
    "babel-core": "^6.22.1",
    "babel-eslint": "^7.1.1",
    "babel-plugin-transform-flow-strip-types": "^6.22.0",
    "babel-preset-es2015": "^6.18.0",
    "babel-preset-react": "^6.16.0",
    "babel-preset-stage-0": "^6.22.0",
    "cross-env": "^3.1.4",
    "eslint": "^3.13.1",
    "eslint-config-standard": "^6.2.1",
    "eslint-config-standard-react": "^4.2.0",
    "eslint-plugin-flowtype": "^2.30.0",
    "eslint-plugin-flowtype-errors": "^2.0.3",
    "eslint-plugin-import": "^2.2.0",
    "eslint-plugin-promise": "^3.4.0",
    "eslint-plugin-react": "^6.9.0",
    "eslint-plugin-standard": "^2.0.1",
    "flow-bin": "^0.37.4",
    "flow-copy-source": "^1.1.0",
    "gh-pages": "^0.12.0"
  },
  "scripts": {
    "build": "babel src -d lib",
    "babel-prepublish": "npm run build",
    "flow-prepublish": "flow-copy-source src lib",
    "prepublish": "npm run babel-prepublish && npm run flow-prepublish",
    "build-examples": "cd examples && npm run clean && npm install",
    "release-examples": "npm run build-examples && gh-pages -d examples/ -m 'Updates v'$npm_package_version --remove node_modules"
  }
}
